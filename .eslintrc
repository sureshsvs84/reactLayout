{
  "env": {
      "browser": false,
      "commonjs": true,
      "es6": true
  },
  "extends": [
      "plugin:react/recommended"
    ],
  "parserOptions": {
      "ecmaFeatures": {
          "experimentalObjectRestSpread": true,
          "jsx": true
      },
      "sourceType": "module"
  },
  "plugins": [
      "react"
  ],
  "rules": {
      "react/jsx-indent":"off",
      "react/prop-types": 0,
      "linebreak-style": [
          0,
          "error",
          "windows"
      ],
      "quotes": [
         "error",            
          "double"
                  
      ],
      "semi": [
          "warn",
          "always"
      ]
  },
  "parser": "babel-eslint"
}
